{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tealdeer",
  "title": "tealdeer",
  "description": "Do tealdeer stuff in Raycast",
  "icon": "command-icon.png",
  "author": "rushi",
  "categories": [
    "Developer Tools"
  ],
  "contributors": [
    "pomdtr"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Pages",
      "subtitle": "tealdeer",
      "description": "Search cheatsheets from the TLDR reference list and your custom ones",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.0",
    "@raycast/utils": "^1.14.0",
    "degit": "^2.8.4",
    "globby": "^14.0.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/degit": "^2.8.6",
    "@types/globby": "^9.1.0",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "format": "prettier --write -l src",
    "publish": "npx @raycast/api@latest publish"
  }
}
